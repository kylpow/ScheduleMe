<UserControl x:Class="ScheduleMe.Views.Buttons.ButtonPersonalInfo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ScheduleMe.Views.Buttons"
             mc:Ignorable="d">
    <Grid>
        <Button Name="btnPersonalInfo"
                Content="Personal Info" 
                HorizontalAlignment="Left" 
                VerticalAlignment="Top" 
                Width="150"
                Style="{StaticResource AccentedSquareButtonStyle}" Click="btnPersonalInfo_Click" 
                />
    </Grid>
</UserControl>
